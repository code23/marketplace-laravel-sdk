{
    "name": "code23/marketplace-sdk",
    "description": "Marketplace SDK",
    "keywords": [
        "code23",
        "marketplace-sdk"
    ],
    "homepage": "https://github.com/code23/marketplace-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Code23",
            "email": "dev@code23.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Code23\\MarketplaceSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code23\\MarketplaceSDK\\Tests\\": "tests"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code23\\MarketplaceSDK\\MarketplaceSDKServiceProvider"
            ],
            "aliases": {
                "MPEAuthentication": "Code23\\MarketplaceSDK\\Facades\\MPEAuthentication"
            }
        }
    }
}
